<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CleanLib.Maui.Themes.Colors.ApiColors">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/CleanLib.Maui;component/Themes/Colors/Colors.xaml"/>
    </ResourceDictionary.MergedDictionaries>

    <Color x:Key="apiBlueColor">#243688</Color>
    <Color x:Key="apiLightBlueColor">#46AFEB</Color>
    <Color x:Key="apiRedColor">#982E5F</Color>
    <Color x:Key="apiGreenColor">#8BB113</Color>
    <Color x:Key="apiTurquiseColor">#119BA4</Color>

    <SolidColorBrush x:Key="apiBlueBrush" Color="{StaticResource apiBlueColor}"/>
    <SolidColorBrush x:Key="apiLightBlueBrush" Color="{StaticResource apiLightBlueColor}"/>
    <SolidColorBrush x:Key="apiRedBrush" Color="{StaticResource apiRedColor}"/>
    <SolidColorBrush x:Key="apiGreenBrush" Color="{StaticResource apiGreenColor}"/>
    <SolidColorBrush x:Key="apiTurquiseBrush" Color="{StaticResource apiTurquiseColor}"/>
</ResourceDictionary>